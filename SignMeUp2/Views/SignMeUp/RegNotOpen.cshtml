@using LangResources;

@model SignMeUp2.Data.Evenemang

@{
    ViewBag.Title = Language.RegistrationClosed;
    Layout = "~/Views/Shared/_LayoutSimple.cshtml";
}

@if (ViewBag.Closed)
{
    <h2>@Language.RegFor @Model.Namn @Language.IsClosed @Model.RegStop.ToShortDateString()</h2>
}
else if (ViewBag.NotOpen)
{
    <h2>@Language.RegFor @Model.Namn @Language.NotYetOpened @Model.RegStop.ToShortDateString()</h2>
}

