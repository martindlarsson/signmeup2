@model IEnumerable<SignMeUp2.DataModel.Evenemang>

@{
    ViewBag.Title = "Lista på alla evenemang";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SignMeUp2 - alla evenemang</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Namn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegStop)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Namn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegStop)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
